{
  "name": "tenji",
  "version": "1.0.2",
  "description": "Converts Japanese text and 点字.",
  "main": "dist/lib/index.js",
  "typings": "dist/lib/index.d.ts",
  "scripts": {
    "build": "npm-run-all build:tsc build:table",
    "build:table": "node ./dist/build/index.js",
    "build:tsc": "tsc",
    "clean": "rimraf dist",
    "test": "node run-jasmine.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:tsc": "tsc --watch",
    "watch:test": "mkdirp dist && watch 'node run-jasmine.js' dist/ --wait=0.2 --verbose"
  },
  "keywords": [
    "tenji",
    "点字",
    "Braille"
  ],
  "author": "uhyo",
  "license": "MIT",
  "devDependencies": {
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.5.2",
    "typescript": "^2.8.1",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.6.1"
  }
}
